= simple_form_for @round_form, url: rounds_path, method: :post do |f|
  = f.input :course_id, collection: Course.pluck(:name, :id), input_html: { "data-iconpos" => "left" }
  = f.input :user_ids, collection: User.pluck(:name, :id), label: "Players", input_html: { multiple: "multiple", "data-native-menu" => "false", "data-iconpos" => "left" }
  = f.submit "Create Round"

